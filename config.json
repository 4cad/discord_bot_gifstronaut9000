{
    "rules": [
        {
            "match_value" : "^!solved",
            "tags" : ["win","celebrate","well done","awesome","excellent","woohoo"]
        },
        {
            "match_value" : "^!unsolved",
            "tags" : ["defeat", "demolished", "depression"]
        },
        {
            "match_value" : "^!writeup",
            "tags" : ["youre the best", "champion", "master", "expert"]
        }
    ]
}
